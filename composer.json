{
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        }
    },
    "description": "The Laravel Framework.",
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "name": "laravel/laravel",
    "prefer-stable": true,
    "require": {
        "php": "^8.2.0",
        "ext-zip": "*",
        "barryvdh/laravel-dompdf": "^2.1",
        "doctrine/dbal": "^3.6",
        "filament/filament": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "honeybadger-io/honeybadger-laravel": "^4.0",
        "intervention/image": "^2.7",
        "laravel/framework": "^11.12",
        "laravel/horizon": "^5.22",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.8",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.12",
        "maatwebsite/excel": "^3.1",
        "mcamara/laravel-localization": "^2.0",
        "meilisearch/meilisearch-php": "^1.0",
        "moneyphp/money": "^4.1",
        "outhebox/laravel-translations": "^0.0.6",
        "whitecube/laravel-cookie-consent": "^1.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.10",
        "barryvdh/laravel-ide-helper": "^3.0",
        "laravel/breeze": "^2.0",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "nunomaduro/larastan": "^2.8",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-faker": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.2",
        "pestphp/pest-plugin-livewire": "^2.0",
        "revolution/sail-db-backup": "^2.0",
        "tightenco/duster": "^1.0",
        "fakerphp/faker": "^1.23",
        "phpunit/phpunit": "^11.0.1"
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "type": "project"
}
